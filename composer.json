{
	"name": "dek-apps/netteform",
	"config": {
		"secure-http": false,
		"fxp-asset": {
			"repositories": {
				"bower-asset/pikaday-time": {
					"type": "bower-vcs",
					"url": "https://github.com/vrestihnat/Pikaday.git"
				}
			},
			"installer-paths": {
				"npm-asset-library": "www/assets/vendor",
				"bower-asset-library": "www/assets/vendor"
			}
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.dek.cz"
		}
	],
	"require": {
		"php": ">= 7.1",
		"nette/utils": "^3.0",
		"nette/forms": "^3.0",
		"nette/application": "^3.0",
		"kdyby/translation": "^3.0",
		"symfony/yaml": "3.4.11",
		"bower-asset/pikaday-time": "dev-master"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"mockery/mockery": "^1.0"
		
	},
	"autoload": {
		"psr-4": {
			"DekApps\\": "src/"
		}
	}
}
